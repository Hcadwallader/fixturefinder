@using FixtureFinder.Models
@model List<Event>
@{
//ViewBag.Title = "FixtureFinder";
}
<div class="row">
    <div class="col-md-12" id="bg">
        <div class="row">
            <div class="col-md-6 col-md-offset-3 panel panel-default panelstyle scrollbar">
        <h3 class="panel-title fontstyling">Results</h3>
        <div class="panel-body">
            @foreach (Event e in Model)
            {
                <p>@e.hometeam Vs @e.awayteam, @e.location @e.date.ToString("h:mm tt dddd d MMMM, yyyy ")
                Distance: @e.distanceFriendly, Travel time: @e.durationFriendly</p>
                <hr />
            }
            
        </div>
    </div>
</div>
    </div>
</div>

